              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
           {{#link-to "application" class="navbar-brand"}}Log My Dive{{/link-to}}
          </div>
         
          <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
              {{#link-to "dives" tagName="li"}}
              	<a style="cursor: pointer">Dives</a>
                
              {{/link-to}}
              {{#link-to "users" tagName="li"}}
                <a {{bind-attr href="view.href"}}>Users</a>
